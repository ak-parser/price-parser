{
  "Logging": {
    "LogLevel": {
      "Default": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information",
        "Microsoft": "Warning"
      }
    }
  },
  "AllowedHosts": "*",
  "CosmosDb": {
    "ConnectionString": "<cosmos-db-connection-string>",
    "DatabaseId": "<warranty-db-name>",
    "RequestTimeout": "<timeout-in-seconds>",
    "MaxRetryAttemptsOnRateLimitedRequests": "<number>",
    "MaxRetryWaitTimeOnRateLimitedRequests": "<time-in-seconds>"
  },
  "EventGrid": {
    "Topics": {
      "VehicleWarranty": {
        "Endpoint": "<api-endpoint>",
        "AccessKey": "<subscription-key-value>"
      }
    }
  }
}